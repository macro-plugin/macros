{
  "private": true,
  "description": "Macro system for JavaScript and TypeScript.",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:update": "jest --updateSnapshot",
    "build": "nx run-many --target=build --exclude *-project",
    "build:force": "nx run-many --target=build --skip-nx-cache --exclude *-project",
    "build:affected": "nx affected --target=build --exclude *-project",
    "version": "changeset version",
    "publish": "changeset publish",
    "graph": "nx graph",
    "coverage": "jest --coverage"
  },
  "author": "Raven Satir",
  "license": "MIT",
  "devDependencies": {
    "@jest/types": "^29.5.0",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.4.0",
    "@swc/jest": "^0.2.24",
    "@types/jest": "^29.4.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "eslint": "^8.36.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.5.0",
    "nx": "15.8.7",
    "rollup": "^3.20.0",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-typescript2": "^0.34.1",
    "tslib": "^2.5.0",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "@changesets/cli": "^2.26.1",
    "@swc/core": "^1.3.40"
  }
}
