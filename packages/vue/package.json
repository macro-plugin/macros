{
  "name": "@macro-plugin/vue",
  "version": "1.0.0",
  "private": true,
  "description": "A simpler way of creating components for vue by using macros",
  "main": "dist/vue.js",
  "module": "dist/vue.mjs",
  "types": "./dist/vue.d.ts",
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "keywords": [
    "vue",
    "vuejs",
    "macro",
    "macros",
    "macro-plugin"
  ],
  "homepage": "https://github.com/macro-plugin/macros/tree/main/packages/vue",
  "repository": {
    "type": "git",
    "url": "https://github.com/macro-plugin/macros.git"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/vue.mjs",
      "require": "./dist/vue.js",
      "types": "./dist/vue.d.ts"
    }
  },
  "author": "Raven Satir",
  "license": "MIT"
}
