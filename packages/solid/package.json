{
  "name": "@macro-plugin/solid",
  "version": "1.0.4",
  "description": "A simpler way of creating components for solid by using macros",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "keywords": [
    "solid",
    "solidjs",
    "macro",
    "macros"
  ],
  "scripts": {
    "build": "rollup -c ../../rollup.config.mjs"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "homepage": "https://github.com/macro-plugin/macros/tree/main/packages/solid",
  "repository": {
    "type": "git",
    "url": "https://github.com/macro-plugin/macros.git"
  },
  "dependencies": {
    "@macro-plugin/core": "1.1.0"
  },
  "author": "Raven Satir",
  "license": "MIT"
}
